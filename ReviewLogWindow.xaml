<Window x:Class="Admin.ReviewLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Admin"
        mc:Ignorable="d"
        Title="ReviewLogWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Rectangle HorizontalAlignment="Left" Height="419" Stroke="Black" VerticalAlignment="Top" Width="792">
            <Rectangle.Fill>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <GradientStop Color="#1D3354" Offset="0.0" />
                    <GradientStop Color="#9ED8DB" Offset="1.0" />
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <TextBox Name="LogFileTextBox" FontSize="14" HorizontalAlignment="Left" Background="#E9FFF9" Height="370" Margin="10,28,0,0" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Top" Width="772" VerticalScrollBarVisibility="Visible"/>
        <Button Name="ExitLogButton" Content="Exit Log" HorizontalAlignment="Left" Margin="707,3,0,0" VerticalAlignment="Top" Width="75" Click="ExitLogButton_Click"/>

    </Grid>
</Window>
